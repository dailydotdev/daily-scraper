  },
  "homepage": "https://github.com/dailydotdev/daily-scraper#readme",
  "dependencies": {
    "@google-cloud/trace-agent": "^5.1.3",
    "dotenv": "^8.2.0",
    "fast-json-stringify": "^2.4.1",
    "fastify": "^2.15.3",
    "fastify-helmet": "^3.0.2",
    "fastify-rate-limit": "^3.0.1",
    "feedparser": "^2.2.10",
    "generic-pool": "^3.7.1",
    "puppeteer": "^7.0.0"
  },
  "devDependencies": {
    "@types/feedparser": "^2.2.3",
    "@types/generic-pool": "^3.1.9",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.26",
    "@types/puppeteer": "^5.4.3",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fastify-static": "^2.7.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "nodemon": "^2.0.7",
    "pino-pretty": "^4.5.0",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
